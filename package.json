{
  "name": "internetmarke",
  "version": "0.4.3",
  "description": "A node implementation to use the Internetmarke web service of Deutsche Post.",
  "keywords": [
    "internetmarke",
    "stamp",
    "voucher",
    "post",
    "letter",
    "1c4a",
    "prod-ws",
    "deutsche-post"
  ],
  "main": "./index.js",
  "types": "./lib/Internetmarke.d.ts",
  "repository": "https://github.com/schaechinger/internetmarke.git",
  "author": "Manuel Sch√§chinger <manuel@schaechinger.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schaechinger/internetmarke/issues"
  },
  "scripts": {
    "clean": "rm -rf lib .nyc_output coverage",
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "test": "NODE_ENV=test mocha -r ts-node/register 'test/**/*.spec.ts'",
    "test:codecov": "nyc --reporter=cobertura npm run test",
    "test:coverage": "nyc npm run test"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "md5": "^2.3.0",
    "moment-timezone": "^0.5.33",
    "soap": "^0.40.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/chai": "^4.2.21",
    "@types/debug": "^4.1.7",
    "@types/md5": "^2.3.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.3",
    "@types/sinon": "10.0.2",
    "chai": "^4.3.4",
    "husky": "^7.0.1",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "rimraf": "3.0.2",
    "sinon": "11.1.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "nyc": {
    "branches": "&gt;80",
    "lines": "&gt;80",
    "functions": "&gt;80",
    "statements": "&gt;80"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
