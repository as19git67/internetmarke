{
  "name": "internetmarke",
  "version": "0.4.0",
  "description": "A node wrapper for the Internetmarke web service of the Deutsche Post.",
  "keywords": [
    "internetmarke",
    "stamp",
    "voucher",
    "post",
    "letter",
    "1c4a",
    "prod-ws",
    "deutsche-post"
  ],
  "main": "./index.js",
  "types": "./lib/Internetmarke.d.ts",
  "repository": "https://github.com/schaechinger/internetmarke.git",
  "author": "Manuel Sch√§chinger <manuel@schaechinger.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schaechinger/internetmarke/issues"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "test": "NODE_ENV=test mocha $(find test -name '*.test.js')",
    "cover": "./node_modules/.bin/istanbul cover mocha $(find test -name '*.test.js') --report lcovonly -- -R spec",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "md5": "^2.3.0",
    "moment-timezone": "^0.5.33",
    "soap": "^0.40.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/debug": "^4.1.7",
    "@types/md5": "^2.3.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "16.4.3",
    "@types/should": "^13.0.0",
    "@types/sinon": "^10.0.2",
    "coveralls": "^3.1.1",
    "husky": "^7.0.1",
    "istanbul": "^0.4.5",
    "license-checker": "25.0.1",
    "mocha": "^9.0.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.12",
    "prettier": "2.3.2",
    "should": "^13.2.3",
    "sinon": "^11.1.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}